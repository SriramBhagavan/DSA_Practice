var isPalindrome = function(head) {
    //find middle
    let slow=fast=head
    while(fast && fast.next){
        slow=slow.next
        fast=fast.next.next
    }


    //reverse the linked list from the middle
    let cur=slow
    let prev=null
   
    while(cur){
         let temp=cur.next
        cur.next=prev
        prev=cur
        cur=temp
    }

    //compare with start and end values of both list
    let firstLinkedlist=head
    let secondLinkedlist=prev
    while(secondLinkedlist){
        if(firstLinkedlist.val !== secondLinkedlist.val){
            return false

        }
         firstLinkedlist=firstLinkedlist.next
    secondLinkedlist=secondLinkedlist.next
    }
   
    return true
    
    
}; 